{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended","prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "camelcase":"error",
        "no-unused-vars":"warn",
        "no-case-declarations":"off"
    }
}
